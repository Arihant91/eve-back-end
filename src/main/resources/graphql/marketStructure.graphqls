extend type Query { getMarketDetails: [GroupDetails]!
}

type MarketItem {
    name: String,
    description: String,
    capacity: Float,
    groupId: Int,
    marketGroupId: Int
    typeId: Int
}

type GroupDetails {
    description : String,
    parentGroupId: Int,
    marketGroupId: Int,
    name: String,
    marketItems: [MarketItem],
    childGroups: [GroupDetails]
}

